<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <VersionPrefix>1.0.0.0</VersionPrefix>
    <TargetFramework>net6.0</TargetFramework>
    <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
    <AssemblyName>LanguageLearning.Application</AssemblyName>
    <PackageId>LanguageLearning.Application</PackageId>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace>LanguageLearning</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\LanguageLearning.Core\LanguageLearning.Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="AppServices\SpeakingQuestions\" />
    <Folder Include="AppServices\WritingQuestions\" />
    <Folder Include="AppServices\VocabularyQuestions\" />
    <Folder Include="AppServices\ListeningQuestions\" />
  </ItemGroup>
</Project>